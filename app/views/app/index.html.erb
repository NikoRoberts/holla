  text-decoration: underline;
}
a :hover {
  text-decoration: underline;
}
img {
  border: 0;
}
hr {
  margin: 1.3em 0.4em;
  border: 0;
  border-bottom: 1px solid #CDCDCC;
  -moz-box-shadow: 0 1px 1px #FCFCFC;
  -webkit-box-shadow: 0 1px 1px #FCFCFC;
  box-shadow: 0 1px 1px #FCFCFC;
}
header {
  display: block;
}
.cta {
  padding: 3px 7px;
  color: #FFF;
  font-weight: bold;
  border: 2px solid #FFF;
